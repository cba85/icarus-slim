{
    "name": "cba85/icarus-slim",
    "description": "Starter kit for PHP Slim framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Clement Barbaza",
            "email": "hello@clementbarbaza.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "cba85/icarus-slim-framework": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:/cba85/icarus-slim-framework.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
},
    "autoload-dev": {
       "psr-4": {
           "Tests\\": "tests/"
       }
   },
    "config": {
       "process-timeout" : 0
   }
}
