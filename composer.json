{
    "name": "cba85/icarus-slim",
    "description": "Starter kit for PHP Slim framework",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Clement Barbaza",
            "email": "hello@clementbarbaza.com"
        }
    ],
    "require": {
        "php": ">=7.0.0",
        "slim/slim": "^3.0",
        "vlucas/phpdotenv": "^2.4",
        "monolog/monolog": "^1.22",
        "slim/csrf": "^0.8.1",
        "slim/flash": "^0.2.0",
        "tuupola/slim-basic-auth": "^2.2",
        "davidepastore/slim-validation": "^0.5.0",
        "samdark/sitemap": "^2.0",
        "slim/twig-view": "^2.2",
        "hassankhan/config": "^0.11.2",
        "kanellov/slim-twig-flash": "^0.2.0",
        "aura/sql": "^2.5",
        "cba85/icarus-slim-framework": "dev-master"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "git@github.com:/cba85/icarus-slim-framework.git"
        }
    ],
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files" : [
            "src/Helpers/app.php"
        ]
},
    "autoload-dev": {
       "psr-4": {
           "Tests\\": "tests/"
       }
   },
    "config": {
       "process-timeout" : 0
   }
}
